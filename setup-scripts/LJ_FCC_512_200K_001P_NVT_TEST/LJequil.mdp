;Run control

integrator               = md 

tinit                    = 0
dt                       = 0.00005
nsteps                   = 100000

comm-mode                = Linear 
nstcomm                  = 100


;Output control

nstxout                 = 100 
nstlog                  = 100
nstcalcenergy		= 1
nstenergy		= 100
nstxout-compressed	= 10000
compressed-x-precision	= 1000


;Neighbor searching

cutoff-scheme		= Verlet
nstlist			= 10
ns_type			= grid
pbc			= xyz
verlet-buffer-tolerance	= 0.005
rlist 			= 0.35

;Electrostatics
coulombtype		= Cut-off
coulombmodifier		= Potential-shift
rcoulomb 		= 0.32

;VanDerWaals
vdwtype			= Cut-off
vdw_modifier		= Potential-Switch
rvdw			= 0.32
rvdw-switch		= 0.3
DispCorr		= no

;Ewald??

;Temperature coupling
tcoupl			= nose-hoover
nsttcouple		= 10
tc-grps			= System
tau_t			= 5
ref_t			= 40


;Pressure coupling
pcoupl			= no
;pcoupltype		= isotropic
;nstpcouple		= 10
;tau_p			= 200
;compressibility		= 3.5e-5
;refcoord-scaling	= no

;Velocity generation
gen_vel			= yes
gen_temp		= 300
gen_seed		= 12

;Other
free_energy		= no
symmetry-averaging 	= no







