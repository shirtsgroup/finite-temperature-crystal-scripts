                        C2sed="/C2 / s/   0   /   $C2Num   /1"
                        N3sed="/N3/ s/   0   /   $N3Num   /1"
                        N4sed="/N4/ s/   0   /   $N4Num   /1"
                        N5sed="/N5/ s/   0   /   $N5Num   /1"
                        N6sed="/N6/ s/   0   /   $N6Num   /1"
                        O7sed="/O7/ s/   0   /   $O7Num   /1"
                        O8sed="/O8/ s/   0   /   $O8Num   /1"
                        O9sed="/O9/ s/   0   /   $O9Num   /1"
			O10sed="/O10/ s/   0   /   $O10Num   /1"
                        H11sed="/H11/ s/   0   /   $H11Num   /1"
                        H12sed="/H12/ s/   0   /   $H12Num   /1"
                        H13sed="/H13/ s/   0   /   $H13Num   /1"
                        H14sed="/H14/ s/   0   /   $H14Num   /1"
                        sed -i "$C1sed" $file  #C1
                        sed -i "$C2sed" $file  #C2
                        sed -i "$N3sed" $file  #N3
                        sed -i "$N4sed" $file  #N4
                        sed -i "$N5sed" $file  #N5
                        sed -i "$N6sed" $file  #N6
                        sed -i "$O7sed" $file  #O7
                        sed -i "$O8sed" $file  #O8
                        sed -i "$O9sed" $file  #O9
                        sed -i "$O10sed" $file  #O10
                        sed -i "$H11sed" $file  #H11
                        sed -i "$H12sed" $file  #H12
                        sed -i "$H13sed" $file  #H13
                        sed -i "$H14sed" $file  #H14
                        C1sed="/C1 / s/   0  /   $C1Num   /1"
                        C2sed="/C2 / s/   0  /   $C2Num   /1"
                        N3sed="/N3/ s/   0  /   $N3Num   /1"
                        N4sed="/N4/ s/   0  /   $N4Num   /1"
                        N5sed="/N5/ s/   0  /   $N5Num   /1"
                        N6sed="/N6/ s/   0  /   $N6Num   /1"
                        O7sed="/O7/ s/   0  /   $O7Num   /1"
                        O8sed="/O8/ s/   0  /   $O8Num   /1"
                        O9sed="/O9/ s/   0  /   $O9Num   /1"
                        O10sed="/O10/ s/   0  /   $O10Num   /1"
                        H11sed="/H11/ s/   0  /   $H11Num   /1"
                        H12sed="/H12/ s/   0  /   $H12Num   /1"
                        H13sed="/H13/ s/   0  /   $H13Num   /1"
                        H14sed="/H14/ s/   0  /   $H14Num   /1"
                        sed -i "$C1sed" $file  #C1
                        sed -i "$C2sed" $file  #C2
                        sed -i "$N3sed" $file  #N3
                        sed -i "$N4sed" $file  #N4
                        sed -i "$N5sed" $file  #N5
                        sed -i "$N6sed" $file  #N6
                        sed -i "$O7sed" $file  #O7
                        sed -i "$O8sed" $file  #O8
                        sed -i "$O9sed" $file  #O9
                        sed -i "$O10sed" $file  #O10
                        sed -i "$H11sed" $file  #H11
                        sed -i "$H12sed" $file  #H12
                        sed -i "$H13sed" $file  #H13
                        sed -i "$H14sed" $file  #H14

                    fi

                ;;

        pyrdna)
                if [ "$paramfile" == "amoeba09" ]; then
                        O1Num=150
                        H2Num=151
                        O3Num=153
                        C4Num=152
                        C5Num=154
                        H6Num=155
                        H7Num=155
                        H8Num=155
                elif [ "$paramfile" == "oplsaa" ]; then
                        C1Num=464
                        C2Num=463
                        C3Num=462
                        C4Num=463
                        C5Num=462
                        N6Num=461
                        H7Num=467
                        H8Num=466
                        H9Num=465
                        H10Num=466
                        H11Num=465
                else
                        echo "Unsupported Potential"
                        exit 1
                fi
		    if [ $(sed -n '4p' $file | awk '{print $6}') == '0' ]; then
                        echo "adding atom types to ${file}..."
                        C1sed="/C1 / s/   0   /   $C1Num   /1"
                        C2sed="/C2 / s/   0   /   $C2Num   /1"
                        C3sed="/C3/ s/   0   /   $C3Num   /1"
                        C4sed="/C4/ s/   0   /   $C4Num   /1"
                        C5sed="/C5/ s/   0   /   $C5Num   /1"
                        N6sed="/N6/ s/   0   /   $N6Num   /1"
                        H7sed="/H7/ s/   0   /   $H7Num   /1"
                        H8sed="/H8/ s/   0   /   $H8Num   /1"
                        H9sed="/H9/ s/   0   /   $H9Num   /1"
                        H10sed="/H10/ s/   0   /   $H10Num   /1"
                        H11sed="/H11/ s/   0  /   $H11Num   /1"
                        sed -i "$C1sed" $file  #C1
                        sed -i "$C2sed" $file  #C2
                        sed -i "$C3sed" $file  #C3
                        sed -i "$C4sed" $file  #C4
                        sed -i "$C5sed" $file  #C5
                        sed -i "$N6sed" $file  #N6
                        sed -i "$H7sed" $file  #H7
                        sed -i "$H8sed" $file  #H8
                        sed -i "$H9sed" $file  #H9
                        sed -i "$H10sed" $file  #H10
                        sed -i "$H11sed" $file  #H11
			C1sed="/C1 / s/   0  /   $C1Num   /1"
                        C2sed="/C2 / s/   0  /   $C2Num   /1"
                        C3sed="/C3/ s/   0  /   $C3Num   /1"
                        C4sed="/C4/ s/   0  /   $C4Num   /1"
                        C5sed="/C5/ s/   0  /   $C5Num   /1"
                        N6sed="/N6/ s/   0  /   $N6Num   /1"
                        H7sed="/H7/ s/   0  /   $H7Num   /1"
                        H8sed="/H8/ s/   0  /   $H8Num   /1"
                        H9sed="/H9/ s/   0  /   $H9Num   /1"
                        H10sed="/H10/ s/   0  /   $H10Num   /1"
                        H11sed="/H11/ s/   0 /   $H11Num   /1"
                        sed -i "$C1sed" $file  #C1
                        sed -i "$C2sed" $file  #C2
                        sed -i "$C3sed" $file  #C3
                        sed -i "$C4sed" $file  #C4
                        sed -i "$C5sed" $file  #C5
                        sed -i "$N6sed" $file  #N6
                        sed -i "$H7sed" $file  #H7
                        sed -i "$H8sed" $file  #H8
                        sed -i "$H9sed" $file  #H9
                        sed -i "$H10sed" $file  #H10
                        sed -i "$H11sed" $file  #H11
                    fi

                ;;



esac

